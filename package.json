{
  "name": "fuseswap-service",
  "version": "0.1.0",
  "description": "node-skeleton",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/server.ts",
    "debug": "node --inspect ./build/server.js",
    "build": "tsc",
    "start-prod": "NODE_ENV=production ts-node-transpile-only ./src/server.ts",
    "doc": "apidoc -i src/routes -o docs/api && apidoc-markdown -p docs/api -o docs/api.md",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "jest",
    "test-unit": "jest ./test/unit",
    "test-integration": "jest ./test/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/node-skeleton.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "assert"
    ],
    "ignore": [
      "docs"
    ]
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services",
    "@controllers": "./src/controllers"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@ethersproject/abi": "^5.6.0",
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.2",
    "@ethersproject/solidity": "^5.6.0",
    "@ethersproject/units": "^5.6.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^27.4.1",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@uniswap/token-lists": "^1.0.0-beta.20",
    "@voltage-finance/sdk": "^1.0.0",
    "@voltage-finance/swap-default-token-list": "^1.0.2",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.1.1",
    "express-paginate": "^1.0.2",
    "express-validator": "^6.14.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "http-status-codes": "^2.2.0",
    "jest": "^27.5.1",
    "lodash.flatmap": "^4.5.0",
    "lodash.floor": "^4.0.4",
    "lodash.get": "^4.4.2",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedi": "^0.10.0",
    "typescript": "^4.6.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@rigwild/apidoc-markdown": "^5.0.0",
    "@types/express-validator": "^3.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/lodash.compact": "^3.0.6",
    "@types/lodash.floor": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "apidoc": "^0.51.0",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "lodash.compact": "^3.0.1",
    "nodemon": "^2.0.15",
    "sinon": "^13.0.1",
    "standard": "^16.0.4",
    "supertest": "^6.2.2",
    "ts-standard": "^11.0.0",
    "tsconfig-paths": "^3.14.1"
  }
}
