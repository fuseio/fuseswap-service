{
  "name": "fuseswap-service",
  "version": "0.1.0",
  "description": "node-skeleton",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/server.ts",
    "debug": "node --inspect ./build/server.js",
    "build": "tsc",
    "start-prod": "NODE_ENV=production ts-node-transpile-only ./src/server.ts",
    "doc": "apidoc -i src/routes -o docs/api && apidoc-markdown -p docs/api -o docs/api.md",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "jest",
    "test-unit": "jest ./test/unit",
    "test-integration": "jest ./test/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/node-skeleton.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "assert"
    ],
    "ignore": [
      "docs"
    ]
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services",
    "@controllers": "./src/controllers"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@ethersproject/abi": "^5.0.9",
    "@ethersproject/address": "^5.0.8",
    "@ethersproject/bignumber": "^5.0.12",
    "@ethersproject/contracts": "^5.0.8",
    "@ethersproject/providers": "^5.0.17",
    "@ethersproject/solidity": "^5.0.7",
    "@ethersproject/units": "^5.0.8",
    "@types/config": "0.0.37",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^26.0.19",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@uniswap/token-lists": "^1.0.0-beta.20",
    "@voltage-finance/sdk": "^1.0.0",
    "@voltage-finance/swap-default-token-list": "^1.0.2",
    "async": "^2.6.1",
    "aws-sdk": "^2.405.0",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "2.7.1",
    "dayjs": "^1.10.4",
    "debug": "2.6.9",
    "eslint-config-prettier": "^7.0.0",
    "express": "4.16.3",
    "express-async-errors": "^2.1.2",
    "express-jwt": "^5.3.1",
    "express-paginate": "^1.0.0",
    "express-validator": "^6.8.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.5.0",
    "graphql-tag": "^2.11.0",
    "http-status-codes": "^2.1.4",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.4.0",
    "lodash.flatmap": "^4.5.0",
    "lodash.floor": "^4.0.4",
    "lodash.get": "^4.4.2",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "methods": "1.1.2",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedi": "^0.8.0",
    "typescript": "^4.1.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@rigwild/apidoc-markdown": "^5.0.0",
    "@types/express-validator": "^3.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/lodash.compact": "^3.0.6",
    "@types/lodash.floor": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/sinon": "^9.0.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "apidoc": "^0.51.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "lodash.compact": "^3.0.1",
    "nodemon": "^1.18.11",
    "sinon": "^9.2.2",
    "standard": "^16.0.4",
    "supertest": "^6.0.1",
    "ts-standard": "^11.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
